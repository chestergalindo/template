{
  "name": "template",
  "private": true,
  "version": "0.7.4",
  "scripts": {
    "test": "is-ci-cli \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "lint": "eslint --ignore-pattern .gitignore --ext .js,.ts,.tsx './src'",
    "prettier": "prettier  --ignore-pattern .gitignore --ext .js,.ts,.tsx,.json './src'",
    "check-types": "tsc",
    "check-format": " npm run prettier -- --list-different",
    "format": "npm run prettier -- --write",
    "validate": "npm-run-all --parallel check-types check-format",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@babel/preset-env": "7.19.3",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.1.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-helmet": "6.1.5",
    "@types/react-portal": "4.0.4",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "@vitejs/plugin-react": "2.1.0",
    "autoprefixer": "10.4.12",
    "axios": "0.27.2",
    "babel-preset-react-app": "10.0.1",
    "cssnano": "5.1.13",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "identity-obj-proxy": "3.0.0",
    "is-ci-cli": "2.2.0",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "jest-runner-eslint": "1.1.0",
    "lint-staged": "13.0.3",
    "lodash": "4.17.21",
    "npm-run-all": "4.1.5",
    "postcss-load-config": "4.0.1",
    "postcss-preset-env": "7.8.2",
    "prettier": "2.7.1",
    "react-content-loader": "6.2.0",
    "react-helmet": "6.1.0",
    "react-icons": "4.4.0",
    "react-portal": "4.2.2",
    "react-query": "3.39.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "tslint-react-a11y": "1.1.0",
    "tslint-react-hooks": "2.2.2",
    "typescript": "4.8.4",
    "vite": "3.1.3",
    "vite-tsconfig-paths": "3.5.1"
  }
}
