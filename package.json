    "postcss-preset-env": "7.4.1",
    "prettier": "2.5.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "tslint-react-a11y": "1.1.0",
    "tslint-react-hooks": "2.2.2",
    "typescript": "4.5.4",
    "vite": "2.8.0"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "react-app"
